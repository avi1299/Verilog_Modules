#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5590965ef030 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x559096637290_0 .var "clk", 0 0;
v0x559096637360_0 .var "reset", 0 0;
v0x559096637430_0 .net "value", 7 0, v0x559096637060_0;  1 drivers
S_0x5590965ef1b0 .scope module, "c1" "counter" 2 21, 3 1 0, S_0x5590965ef030;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "reset"
P_0x5590965ef380 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0x559096625e90_0 .net "clk", 0 0, v0x559096637290_0;  1 drivers
v0x559096637060_0 .var "out", 7 0;
v0x559096637140_0 .net "reset", 0 0, v0x559096637360_0;  1 drivers
E_0x559096625620 .event edge, v0x559096637140_0;
E_0x559096626b00 .event posedge, v0x559096625e90_0;
    .scope S_0x5590965ef1b0;
T_0 ;
    %wait E_0x559096626b00;
    %load/vec4 v0x559096637060_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x559096637060_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5590965ef1b0;
T_1 ;
    %wait E_0x559096625620;
    %load/vec4 v0x559096637140_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %cassign/vec4 v0x559096637060_0;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x559096637060_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5590965ef030;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559096637360_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559096637290_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5590965ef030;
T_3 ;
    %vpi_call 2 6 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5590965ef030 {0 0 0};
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559096637360_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559096637360_0, 0, 1;
    %delay 540, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5590965ef030;
T_4 ;
    %delay 1, 0;
    %load/vec4 v0x559096637290_0;
    %nor/r;
    %store/vec4 v0x559096637290_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x5590965ef030;
T_5 ;
    %vpi_call 2 24 "$monitor", "At time %t, value = %h (%0d)", $time, v0x559096637430_0, v0x559096637430_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
